!!!
%html
    %head
        %meta(charset="utf-8")
        %meta(name="viewport" content="width=device-width, initial-scale=1.0")
        %meta(name="renderer" content="webkit")
        %meta(http-equiv="Cache-Control" content="no-siteapp")
        %title #{_title}
        %meta(name="keywords" content="#{_keywords}")
        %meta(name="description" content="#{_description}")
        %link(rel="shortcut icon" href="favicon.ico")
        %link(href="/css/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet")
        %link(href="/css/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet")
        %link(href="/css/plugins/iCheck/custom.css" rel="stylesheet")
        %link(href="/css/plugins/toastr/toastr.min.css" rel="stylesheet")
        %link(href="/css/animate.min.css" rel="stylesheet")
        %link(href="/css/style.min862f.css?v=4.1.0" rel="stylesheet")
        %link(href="/css/m.css" rel="stylesheet")
        - block otherCss
            / block otherCss
    %body.gray-bg
        - block body
            / block body
        
        %script(src="/js/jquery.min.js?v=2.1.4")
        %script(src="/js/bootstrap.min.js?v=3.3.6")
        %script(src="/js/plugins/toastr/toastr.min.js")
        %script(src="/js/plugins/iCheck/icheck.min.js")
        %script(src="/js/content.min.js?v=1.0.1")
        :javascript
            var lang = {
                adminIndex:{
                    option:'#{lang.adminIndex.option}',
                    update:'#{lang.adminIndex.update}',
                    delete:'#{lang.adminIndex.delete}',
                    create:'#{lang.adminIndex.create}',
                }
            };
        %script(src="/js/admine.js?1")
            
        - block otherScript
            / block otherScript
        